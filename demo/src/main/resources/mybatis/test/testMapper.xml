<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.test.TestMapper">
    <select id="selectTest"  resultType="TestVo">
        SELECT 'GOD' AS NAME
    </select>
    <select id="getTestData" resultType="DbTestVo">
        select
            title,
            name
        from
            DB_TEST
    </select>

    <select id="getParamTest" resultType="DbTestVo">
        select
            title,
            name
        from
            DB_TEST
        where 
            name = #{name}
    </select>
</mapper>